    <div role="main" class="ui-content" data-type="horizontal">
        <fieldset class="ui-grid-d">
            <div class="ui-block-a"><button type="button" data-theme="a" class="start">Start</button></div>
            <div class="ui-block-b"><button type="button" data-theme="b" class="reset">Init</button></div>
            <div class="ui-block-c"><button type="button" data-theme="c" class="stop" >Stop</button></div>
            <div class="ui-block-d"><button type="button" data-theme="d" class="clean">Del</button></div>
            <div class="ui-block-e"><button type="button" data-theme="e" class="mode">Mod</button></div>
        </fieldset>
    </div>


    <div role="main_asdf" class="ui-content" data-type="horizontal">
        <fieldset class="ui-grid-d">
            <div class="ui-block-a"><button type="button" data-theme="a" class="start">asdf</button></div>
            <div class="ui-block-b"><button type="button" data-theme="b" class="reset">asdf</button></div>
            <div class="ui-block-c"><button type="button" data-theme="c" class="stop" >asdf</button></div>
            <div class="ui-block-d"><button type="button" data-theme="d" class="clean">asdf</button></div>
            <div class="ui-block-e"><button type="button" data-theme="e" class="mode">asdf</button></div>
        </fieldset>
    </div>


    <div role="test" class="ui-content" data-type="horizontal">
        <fieldset class="ui-grid-d">
            <div class="ui-block-a">asdf</div>
            <div class="ui-block-b">asdf</div>
            <div class="ui-block-c">asdf</div>
            <div class="ui-block-d">asdf</div>
            <div class="ui-block-e">asdf</div>
        </fieldset>
    </div>


    <div class="ui-btn ui-input-btn ui-shadow">
        The Button
        <input type="button" data-corners="false" data-enhanced="true" value="The Button"></input>
    </div>

    <div id="buttons" class="ui-content" data-role="controls" data-type="horizontal" data-mini="true">
        <input type="button" data-mini="true" data-inline="true" class="start" name="start" value="Start" onclick="simulationStart();" />
        <input type="button" data-mini="true" data-inline="true" class="reset" name="reset" value="Init" onclick="simulationReset();" />
        <input type="button" data-mini="true" data-inline="true" class="stop" name="stop"  value="Stop"  onclick="simulationStop();" />
        <input type="button" data-mini="true" data-inline="true" class="clean" name="stop"  value="Del"  onclick="simulationClean(true);" />
        <input type="button" data-mini="true" data-inline="true" class="mode" name="mode"  value="Mod"  onclick="changeMode();" />
    </div>

